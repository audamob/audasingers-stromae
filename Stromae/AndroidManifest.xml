<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.audamob.audasingers.stromae"
    android:versionCode="101"
    android:versionName="@string/app_version" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:logo="@drawable/audamob_version_deux_ic_action_sliding_menu"
        android:icon="@drawable/logo_audamob_version_deux_ic_launcher"
        android:screenOrientation="portrait"
        android:theme="@style/myTheme" >

        <!-- Reciever -->
        <receiver android:name="BootReciver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" >
                </action>
            </intent-filter>
        </receiver>
        <!-- Service -->
        <service
            android:name="com.audamob.audasingers.stromae.view.services.impl.ServiceNewsUpdater"
            android:enabled="true" >
            <intent-filter>
                <action android:name="ServiceNewsUpdater" >
                </action>
            </intent-filter>
        </service>
        <service
            android:name="com.audamob.audasingers.stromae.view.services.impl.ServiceVideoUpdater"
            android:enabled="true" >
            <intent-filter>
                <action android:name="ServiceVideoUpdater" >
                </action>
            </intent-filter>
        </service>
        <service
            android:name="com.audamob.audasingers.stromae.view.services.impl.ServiceMusicUpdater"
            android:enabled="true" >
            <intent-filter>
                <action android:name="ServiceMusicUpdater" >
                </action>
            </intent-filter>
        </service>
        <!-- Activity -->
        <activity
            android:name="com.audamob.audasingers.stromae.activity.Home"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.audamob.audasingers.stromae.activity.PlayList_Interface"
            android:logo="@drawable/audamob_version_deux_back"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.audamob.audasingers.stromae.activity.EditPlaylist"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.audamob.audasingers.stromae.activity.LyricsPage"
            android:logo="@drawable/audamob_version_deux_back"
            android:screenOrientation="portrait"
            android:theme="@style/myTheme" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.MusicPlayer"
            android:logo="@drawable/audamob_version_deux_back"
            android:screenOrientation="portrait"
            android:theme="@style/myTheme" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.NewsPage"
            android:label="News"
            android:logo="@drawable/audamob_version_deux_back"
            android:theme="@style/myTheme" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.PlayerVideoLyrics"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.SelectPlaylist"
            android:label="Select Playlist"
            android:logo="@drawable/audamob_version_deux_back"
            android:theme="@style/myTheme" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.Videos_Interface"
            android:logo="@drawable/audamob_version_deux_back"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.audamob.audasingers.stromae.fragment.MainContainerActivityTest"
            android:label="@string/second_title"
            android:logo="@drawable/audamob_version_deux_ic_action_sliding_menu"
            android:theme="@style/myTheme" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.NewPlaylist"
            android:logo="@drawable/audamob_version_deux_back"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name="com.audamob.audasingers.stromae.activity.DialogActivity"
            android:logo="@drawable/audamob_version_deux_back"
            android:theme="@android:style/Theme.Dialog" />

        <!-- WIDDIT -->
        <activity
            android:name="com.widdit.lockScreen.activities.MainActivity"
            android:allowTaskReparenting="true"
            android:configChanges="keyboardHidden|keyboard"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:taskAffinity="com.widdit"
            android:theme="@android:style/Theme.Black.NoTitleBar"
            android:windowSoftInputMode="stateVisible|adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT" />

                <category android:name="android.intent.category.MONKEY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.widdit.lockScreen.activities.TermsAndConditionsActivity"
            android:allowTaskReparenting="true"
            android:screenOrientation="portrait"
            android:taskAffinity="com.widdit"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name="com.widdit.lockScreen.activities.SettingsActivity"
            android:screenOrientation="portrait"
            android:taskAffinity="com.widdit"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />

        <service android:name="com.widdit.lockScreen.service.HomeBaseService" />
        <service android:name="com.widdit.lockScreen.service.DataService" />
        <service android:name="com.widdit.lockScreen.service.BackgroundSoundService" />

        <receiver android:name="com.widdit.lockScreen.service.receivers.ScreenStateAndBootCompletedReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.widdit.lockScreen.receivers.SMSNotificationsReceiver"
            android:enabled="true" >
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.PhoneCallReciever" >
            <intent-filter android:priority="999" >
                <action android:name="android.intent.action.PHONE_STATE" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.OutgoingPhoneCallReciever" >
            <intent-filter android:priority="999" >
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.NetworkStateReceiver" >
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.BreakingMessageReceiver" >
            <intent-filter>
                <action android:name="com.widdit.homebase.BREAKING_MESSAGE" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.PackageAddedReceiver" >
            <intent-filter android:priority="999" >
                <action android:name="android.intent.action.PACKAGE_ADDED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.lockScreen.receivers.PackageRemovedReceiver" >
            <intent-filter android:priority="999" >
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.widdit.lockScreen.receivers.WidditGCMBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="." />
            </intent-filter>
        </receiver>
        <receiver android:name="com.widdit.shell.AlarmReceiver" >
        </receiver>

        <meta-data
            android:name="PUBLISHER_ID"
            android:value="80042" />
        <meta-data
            android:name="HOME_BASE_ID"
            android:value="820237" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />

    <!-- WIDDIT -->
    <!-- Use to access the internet. -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Use to launch the lock screen on boot. -->
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <!-- Use to check if there is an internet connection. -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>